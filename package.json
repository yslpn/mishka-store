{
  "name": "mishka",
  "homepage": "./",
  "version": "17.0.0",
  "description": "Личный проект «Мишка» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "10.4.x",
    "browser-sync": "2.28.x",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^8.0.0",
    "gulp-less": "5.0.x",
    "gulp-minify": "^3.1.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "9.0.x",
    "gulp-posthtml": "^3.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "3.0.x",
    "gulp-svgstore": "^9.0.0",
    "gulp-webp": "^4.0.1",
    "posthtml-include": "^1.7.4",
    "rimraf": "^4.3.0",
    "stylelint": "^15.2.0",
    "stylelint-config-htmlacademy": "1.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/less/**/*.less\" --syntax less",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
